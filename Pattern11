import java.util.*;
public class Pattern11 {
    public static void main(String[] args) 
    {
        System.out.println("Enter a no.");
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        
        int p = 1;
        for(int i = 1;i<=n;i++)
        {
            for(int j = 1;j<=i;j++)
            {
                System.out.print(p+" ");
                p++;
            }
            System.out.println();
        }
    }
}
