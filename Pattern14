import java.util.*;

public class Pattern15 {
    public static void main(String[]args)
    {
        System.out.println("Enter a no.");
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        for(int i = 1;i<=10;i++)
        {
            System.out.println(n+" * "+i+" = "+n*i);
        }
       
    }
}
